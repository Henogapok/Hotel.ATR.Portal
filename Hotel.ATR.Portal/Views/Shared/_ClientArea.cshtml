
<div class="client-area">
    <div class="container">
        <div class="row">
            <div class="client-carousel">
                @foreach (Client item in Model.Clients){
                    <div class="single-client">
                        <div class="client-image">
                            <a href="#">
                                <img src="@item.PathToImage" alt="" class="p-img">
                                <img src="@item.PathToHoveredImage" alt="" class="s-img">
                            </a>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>